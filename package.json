{
  "name": "xbvr",
  "private": true,
  "scripts": {
    "serve": "cd ui && vue-cli-service serve",
    "build": "cd ui && vue-cli-service build",
    "lint": "cd ui && vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "i18n:extract": "yarn vue-i18n-extract report -v './ui/src/**/*.?(js|vue)' -l './ui/src/locales/*.?(js|json)'",
    "dev": "concurrently 'go generate' 'npm:dev:ui' 'npm:dev:go'",
    "dev:go": "air",
    "dev:ui": "cd ui && vue-cli-service build --watch --silent"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.4",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@mdi/font": "5.9.55",
    "buefy": "0.9.20",
    "bulma-extensions": "6.2.7",
    "date-fns": "2.16.1",
    "ky": "0.27.0",
    "pretty-bytes": "5.5.0",
    "videojs-hotkeys": "0.2.27",
    "videojs-vr": "1.7.1",
    "vue": "2.6.12",
    "vue-debounce": "2.6.0",
    "vue-gallery": "2.0.1",
    "vue-global-events": "1.2.1",
    "vue-i18n": "8.22.4",
    "vue-load-image": "0.1.12",
    "vue-router": "3.4.9",
    "vue-slider-component": "3.2.11",
    "vue-star-rating": "1.7.0",
    "vuex": "3.6.2",
    "wampy": "6.3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-eslint": "4.5.12",
    "@vue/cli-service": "4.5.12",
    "@vue/eslint-config-standard": "6.0.0",
    "babel-eslint": "10.1.0",
    "concurrently": "5.3.0",
    "eslint": "7.16.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "7.3.0",
    "less": "3.12.2",
    "less-loader": "6.2.0",
    "sass": "1.32.12",
    "sass-loader": "9.0.3",
    "simple-progress-webpack-plugin": "1.1.2",
    "vue-cli-plugin-i18n": "1.0.1",
    "vue-i18n-extract": "1.1.11",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
